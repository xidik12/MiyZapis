# Environment Configuration
NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_WS_URL=ws://localhost:5000

# Telegram Configuration
VITE_TELEGRAM_BOT_TOKEN=your_bot_token_here
VITE_TELEGRAM_PAYMENT_PROVIDER_TOKEN=your_payment_provider_token_here

# Production URLs (override for production)
# VITE_API_URL=https://api.yourdomain.com/api
# VITE_WS_URL=wss://api.yourdomain.com

# App Configuration
VITE_APP_NAME=Booking Platform
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional service booking platform for Telegram

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true

# Debug Options (development only)
VITE_DEBUG_WEBSOCKET=false
VITE_DEBUG_API_CALLS=false
VITE_MOCK_TELEGRAM_API=false